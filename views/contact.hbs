<section id="showcase">
    <h2>Contact List</h2>
    <div id="content-contact">
        <table>
        <tr>
            <th>&nbsp;</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
        {{#each contacts}}
        <tr>
            <td><button name="del" data-id="{{id}}">X</button></td>
            <td>{{first}}</td>
            <td>{{last}}</td>
            <td>{{email}}</td>
        </tr>
        {{/each}}
        </table>

        <br><br>
        <p>Insert New Contact</p>
        <form>
            <input type="text" placeholder="firstname" name="contact_first"> &nbsp; 
            <input type="text" placeholder="lastname" name="contact_last"> &nbsp;
            <input type="text" placeholder="email" name="contact_email"> &nbsp; 
            <button type="submit">Submit</button>
        </form>
    </div>
</section>